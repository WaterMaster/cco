<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.buaa.mooc.entity.Group" lazy="false" table="group">
        <id name="gid" type="integer" column="gid">
            <generator class="native"/>
        </id>
        <property name="manager_sid" type="integer" column="manager"/>
        <property name="information" type="string" column="information"/>
        <property name="gname" type="string" column="gname"/>
        <property name="cid" type="integer" column="cid"/>
    </class>
</hibernate-mapping>