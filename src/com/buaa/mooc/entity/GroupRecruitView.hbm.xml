<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.buaa.mooc.entity.GroupRecruitView" lazy="false" table="group_recruit_view">
        <id name="grid" type="integer" column="grid">
            <generator class="native"/>
        </id>
        <property name="s_count" type="java.lang.Long" column="s_count"/>
        <property name="recruit_information" type="string" column="recruit_information"/>
        <property name="group_name" type="string" column="group_name"/>
        <property name="cid" type="integer" column="cid"/>
    </class>
</hibernate-mapping>