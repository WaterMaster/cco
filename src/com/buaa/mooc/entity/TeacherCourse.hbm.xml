<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.buaa.mooc.entity.TeacherCourse" lazy="false" table="teacher_course">
        <composite-id name="pk" class="com.buaa.mooc.entity.TeacherCoursePK">
            <key-property name="tid" type="integer" column="tid"/>
            <key-property name="cid" type="integer" column="cid"/>
        </composite-id>
    </class>
</hibernate-mapping>